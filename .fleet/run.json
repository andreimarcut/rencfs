{
    "configurations": [
        {
            "type": "cargo",
            "name": "rencfs",
            "cargoArgs": ["run"],
            "executableArgs": ["mount", "-m", "$USER_HOME$/rencfs/mnt", "-d", "$USER_HOME$/rencfs/data", "-u"],
            "environment": {
                "RENCFS_PASSWORD": "a",
                "RUST_LOG": "none,rencfs=info",
            },
            "workingDir": "$PROJECT_DIR$",
        },
    ]
}